                <objectClass
                      name="gama.ui.navigator.view.contents.WrappedProject">
                </objectClass>
                <objectClass
                      name="gama.ui.navigator.view.contents.WrappedFolder">
                </objectClass>
                <objectClass
                      name="gama.ui.navigator.view.contents.TopLevelFolder">
                </objectClass>
                <objectClass
                      name="gama.ui.navigator.view.contents.WrappedModelContent">
                </objectClass>
                <objectClass
                      name="gama.ui.navigator.view.contents.WrappedExperimentContent">
                </objectClass>
                <objectClass
                      name="gama.ui.navigator.view.contents.WrappedSyntacticContent">
                </objectClass>
                <objectClass
                      name="gama.ui.navigator.view.contents.WrappedLink">
                </objectClass>
             </or>
          </enablement>
       </decorator>
	</extension>
