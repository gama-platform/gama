<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
            contributorClass="gama.ui.editor.GamlEditorActionContributor"
            default="true"
            extensions="gaml,experiment"
            icon="platform:/plugin/gama.ui.shared/icons/navigator/files/file.model.png"
            id="gaml.compiler.Gaml"
            name="Gaml Editor">
         <contentTypeBinding
               contentTypeId="gama.ui.gaml.type">
         </contentTypeBinding>
      </editor>
   </extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="gaml.compiler.Gaml.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="gaml.compiler.Gaml"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
		<definition id="gaml.compiler.Gaml.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName"
						value="gaml.compiler.Gaml"
						forcePluginActivation="true"/>
				</with>
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="gaml.compiler.Gaml"
			name="Gaml">
			<keywordReference id="gaml.compiler.ui.keyword_Gaml"/>
		</page>
		<page
			category="gaml.compiler.Gaml"
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="gaml.compiler.Gaml.coloring"
			name="Syntax Coloring">
			<keywordReference id="gaml.compiler.ui.keyword_Gaml"/>
		</page>
		<page
			category="gaml.compiler.Gaml"
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="gaml.compiler.Gaml.templates"
			name="Templates">
			<keywordReference id="gaml.compiler.ui.keyword_Gaml"/>
		</page>
  <page
        category="gaml.compiler.Gaml"
        class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
        id="gaml.compiler.Gaml.compiler.preferencePage"
        name="Compiler">
     <keywordReference
           id="gaml.compiler.ui.keyword_Gaml">
     </keywordReference>
  </page>
  <page
        category="gaml.compiler.Gaml"
        class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
        id="gaml.compiler.Gaml.refactoring"
        name="Refactoring">
     <keywordReference
           id="gaml.compiler.ui.keyword_Gaml">
     </keywordReference>
  </page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="gaml.compiler.Gaml"
			name="Gaml">
			<keywordReference id="gaml.compiler.ui.keyword_Gaml"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
  <page
        category="gaml.compiler.Gaml"
        class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
        id="gaml.compiler.Gaml.compiler.propertyPage"
        name="Compiler">
     <keywordReference
           id="gaml.compiler.ui.keyword_Gaml">
     </keywordReference>
     <enabledWhen>
        <adapt
              type="org.eclipse.core.resources.IProject">
        </adapt>
     </enabledWhen>
     <filter
           name="projectNature"
           value="org.eclipse.xtext.ui.shared.xtextNature">
     </filter>
  </page>
	</extension>
 <extension
       point="org.eclipse.ui.handlers">
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
          commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
          commandId="gaml.compiler.Gaml.validate">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
          commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
          commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
       <activeWhen>
          <and>
             <reference
                   definitionId="gaml.compiler.Gaml.XtextEditor.opened">
             </reference>
             <iterate>
                <adapt
                      type="org.eclipse.xtext.ui.editor.outline.IOutlineNode">
                </adapt>
             </iterate>
          </and>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
          commandId="org.eclipse.xtext.ui.editor.FindReferences">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
          commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
          commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
    <handler
          class="org.eclipse.ui.internal.handlers.WidgetMethodHandler:paste"
          commandId="org.eclipse.ui.edit.paste">
       <activeWhen>
          <with
                variable="activeFocusControlId">
             <equals
                   value="search">
             </equals>
          </with>
       </activeWhen>
    </handler>
    <handler
          class="org.eclipse.ui.internal.handlers.WidgetMethodHandler:copy"
          commandId="org.eclipse.ui.edit.copy">
       <activeWhen>
          <with
                variable="activeFocusControlId">
             <equals
                   value="search">
             </equals>
          </with>
       </activeWhen>
    </handler>
    <handler
          class="org.eclipse.ui.internal.handlers.WidgetMethodHandler:cut"
          commandId="org.eclipse.ui.edit.cut">
       <activeWhen>
          <with
                variable="activeFocusControlId">
             <equals
                   value="search">
             </equals>
          </with>
       </activeWhen>
    </handler>
    <handler
          class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
          commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
       <activeWhen>
          <reference
                definitionId="gaml.compiler.Gaml.Editor.opened">
          </reference>
       </activeWhen>
    </handler>
 </extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="gaml.compiler.ui.keyword_Gaml"
			label="Gaml"/>
	</extension>

	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Trigger expensive validation"
			id="gaml.compiler.Gaml.validate"
			name="Validate">
		</command>
		<!-- copy qualified name -->
		<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
		</command>
		<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
		</command>
  <command
        description="Open the quick outline."
        id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
        name="Quick Outline">
  </command>
  <command
        defaultHandler="gama.ui.editor.CleanBuildCommand"
        id="ummisco.command.clean.build"
        name="Clean and Build All">
  </command>
	</extension>

	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			 <command
				 commandId="gaml.compiler.Gaml.validate"
				 style="push"
				 tooltip="Trigger expensive validation">
			<visibleWhen checkEnabled="false">
				<reference
					definitionId="gaml.compiler.Gaml.Editor.opened">
				</reference>
			</visibleWhen>
		</command>
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="gaml.compiler.Gaml.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="gaml.compiler.Gaml.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
  <menuContribution
        locationURI="popup:#TextEditorContext?endof=group.find">
     <command
           commandId="org.eclipse.xtext.ui.editor.FindReferences">
        <visibleWhen
              checkEnabled="false">
           <reference
                 definitionId="gaml.compiler.Gaml.Editor.opened">
           </reference>
        </visibleWhen>
     </command>
  </menuContribution>
  <menuContribution
        allPopups="false"
        locationURI="menu:org.eclipse.ui.main.menu">
     <dynamic
           class="gama.ui.editor.EditorMenu"
           id="gama.ui.modeling.dynamic5">
        <visibleWhen
              checkEnabled="false">
           <reference
                 definitionId="gaml.compiler.Gaml.Editor.opened">
           </reference>
        </visibleWhen>
     </dynamic>
  </menuContribution>
  <menuContribution
        allPopups="false"
        locationURI="menu:file?before=save.ext">
     <command
           commandId="ummisco.command.clean.build"
           disabledIcon="platform:/plugin/gama.ui.shared/icons/build/build.all_disabled.png"
           icon="platform:/plugin/gama.ui.shared/icons/build/build.all.png"
           label="Clean and Validate All"
           style="push">
        <visibleWhen
              checkEnabled="true">
        </visibleWhen>
     </command>
  </menuContribution>
  <menuContribution
        allPopups="false"
        locationURI="menu:editorsMenu?after=org.eclipse.ui.edit.text.zoomOut">
     <dynamic
           class="gama.ui.editor.EditorPresentationMenu"
           id="gama.ui.modeling.dynamic2">
     </dynamic>
  </menuContribution>
  <menuContribution
        locationURI="popup:#TextEditorContext?after=group.open">
     <command
           commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
           style="push"
           tooltip="Open Quick Outline">
        <visibleWhen
              checkEnabled="false">
           <reference
                 definitionId="gaml.compiler.Gaml.Editor.opened">
           </reference>
        </visibleWhen>
     </command>
  </menuContribution>
  <menuContribution
        locationURI="popup:#TextEditorContext?after=group.edit">
     <command
           commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
           style="push">
        <visibleWhen
              checkEnabled="false">
           <reference
                 definitionId="gaml.compiler.Gaml.Editor.opened">
           </reference>
        </visibleWhen>
     </command>
  </menuContribution>
	</extension>

	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="gaml">
		</parser>
  <parser
        class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
        type="experiment">
  </parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
		<resourceServiceProvider
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="gaml">
		</resourceServiceProvider>
        <resourceServiceProvider
              class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
              uriExtension="experiment">
        </resourceServiceProvider>
    </extension>
    
	<!-- marker definitions for gaml.compiler.Gaml -->
	<extension
			id="gaml.check.fast"
			name="Gaml Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="gaml.check.normal"
			name="Gaml Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="gaml.check.expensive"
			name="Gaml Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="gaml"/>
	</extension>
	<!-- Quick Outline -->
	<!-- quickfix marker resolution generator for gaml.compiler.Gaml -->
	<extension
			point="org.eclipse.ui.ide.markerResolution">
		<markerResolutionGenerator
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="gama.ui.editor.gaml.check.fast">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="gama.ui.editor.gaml.check.normal">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
		<markerResolutionGenerator
			class="gaml.compiler.ui.GamlExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
			markerType="gama.ui.editor.gaml.check.expensive">
			<attribute
				name="FIXABLE_KEY"
				value="true">
			</attribute>
		</markerResolutionGenerator>
	</extension>
 <extension
       point="org.eclipse.ui.editors.documentProviders">
    <provider
