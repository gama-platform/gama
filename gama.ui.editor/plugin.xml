		<fileTypes
			extension="gaml"
			type="text">
		</fileTypes>
		<fileTypes
			extension="experiment"
			type="text">
		</fileTypes>
	</extension>
	    <extension point="org.eclipse.ui.decorators">
           <decorator
                 adaptable="true"
                 class="gaml.compiler.ui.decorators.GamlDecorator"
                 id="gama.light.decorator"
                 label="Gaml Errors"
                 lightweight="true"
                 location="TOP_LEFT"
                 state="true">
          <description>
             Aggregates errors in resources to folders and projects.
          </description>
          <enablement>
             <or>
                <objectClass
                      name="org.eclipse.core.resources.IFile">
